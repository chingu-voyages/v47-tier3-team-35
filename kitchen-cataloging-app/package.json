{
  "name": "kitchen-cataloging-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "secrets": "npx ts-node ./scripts/generateEnv.ts",
    "migrate:mongo": "dotenv -e .env.local -- npx prisma db push --schema ./app/prisma/schema.prisma",
    "postinstall": "prisma generate --schema ./app/prisma/schema.prisma",
    "dev": "npm run postinstall && npm run migrate:mongo  && npx next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "turbo": "npm run postinstall && npm run migrate:mongo && npx next dev --turbo",
    "seed": "npx ts-node ./app/prisma/mock/seed.ts"
  },
  "dependencies": {
    "@clerk/nextjs": "^4.29.2",
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@fontsource/roboto": "^5.0.8",
    "@mui/icons-material": "^5.15.6",
    "@mui/material": "^5.15.6",
    "@prisma/client": "^5.8.1",
    "bson-objectid": "^2.0.4",
    "dotenv-cli": "^7.3.0",
    "lodash": "^4.17.21",
    "mongodb": "6.3",
    "next": "14.1.0",
    "react": "^18",
    "react-dom": "^18",
    "react-intersection-observer": "^9.5.3",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.10.8",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "dotenv": "^16.4.1",
    "eslint": "^8",
    "eslint-config-next": "14.0.4",
    "postcss": "^8",
    "prisma": "^5.8.1",
    "tailwindcss": "^3.3.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
